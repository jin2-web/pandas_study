from bs4 import BeautifulSoup
import requests
import re
import pandas as pd


url = 'https://search.naver.com/search.naver?where=nexearch&sm=top_hty&fbm=0&ie=utf8&query=%EC%98%81%ED%99%94%EC%88%9C%EC%9C%84&ackey=6w8jzbgk'
